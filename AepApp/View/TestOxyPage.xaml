<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AepApp.View.TestOxyPage"
             xmlns:roxv="clr-namespace:Rox;assembly=Rox.Xamarin.Video.Portable">
    <ContentPage.Content>
        <StackLayout>
       <Button BackgroundColor="Fuchsia" Clicked="Handle_Clicked" HorizontalOptions="Center" VerticalOptions="Center" WidthRequest="100" HeightRequest="100"/>
       <roxv:VideoView AutoPlay="false" x:Name="MV" LoopPlay="false" FullScreen="true" ShowController="true" VerticalOptions="Fill" HorizontalOptions="Fill" HeightRequest="300"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>